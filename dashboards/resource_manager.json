          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT t1.VO, t2.Previous, t1.Projected, (IFNULL(t1.Projected, 0) - IFNULL(t2.Previous, 0)) as 'Difference'\n\nFROM\n\n(SELECT\n    VO,\n    SUM(WallDuration*Processors)/2628000 / day(max(UpdateTime)) * day(date(last_day(max(UpdateTime)))) as 'Projected'\nFROM VSummaries\nWHERE Site in ($All_Sites) and VO in ($All_VOs) and Year = YEAR(CURRENT_DATE) and Month = MONTH(CURRENT_DATE)\nGROUP BY VO) as t1 \n\nLEFT OUTER JOIN\n\n(SELECT\n    VO,\n    SUM(WallDuration*Processors)/2628000  as 'Previous'\nFROM VSummaries\nWHERE Site in ($All_Sites) and VO in ($All_VOs) and Year = YEAR(CURRENT_DATE) and Month = MONTH(CURRENT_DATE)-1\nGROUP BY VO) as t2\n\nON\n\nt1.VO=t2.VO\n\nUNION\n\nSELECT t2.VO, t2.Previous, t1.Projected, (IFNULL(t1.Projected, 0) - IFNULL(t2.Previous, 0)) as 'Difference'\n\nFROM\n\n(SELECT\n    VO,\n    SUM(WallDuration*Processors)/2628000 / day(max(UpdateTime)) * day(date(last_day(max(UpdateTime)))) as 'Projected'\nFROM VSummaries\nWHERE Site in ($All_Sites) and VO in ($All_VOs) and Year = YEAR(CURRENT_DATE) and Month = MONTH(CURRENT_DATE)\nGROUP BY VO) as t1\n\nRIGHT OUTER JOIN \n\n(SELECT\n    VO,\n    SUM(WallDuration*Processors)/2628000  as 'Previous'\nFROM VSummaries\nWHERE Site in ($All_Sites) and VO in ($All_VOs) and Year = YEAR(CURRENT_DATE) and Month = MONTH(CURRENT_DATE)-1\nGROUP BY VO) as t2\n\nON\n\nt1.VO=t2.VO;\n",
